.profit_add{background: #fff; padding:0px 13px;font-size:14px;color:#333;}
.profit_add .profit_up{height: 45px;line-height: 45px;}
.profit_up_right{float:right;font-size: 22px;
    color: #efc427;}
.profit_up_right text{font-size:14px;color:#333;padding-left:5px;}
.profit_add .profit_up h3{float:left;font-size: 15px;}
.profit_add .profit_up span{float:right;font-size: 22px;color:#efc427;}
.profit_add .profit_up span i{font-size:12px;color:#212121; padding-left: 3px;}
.profit_add .profit_un{ padding: 18px 0;}
.profit_add .profit_un li{width:50%;float: left;text-align: center;font-size: 18px;color:#efc427;position: relative;}

.profit_add .profit_un li i{font-size: 12px;color:#949494;}
.profit_add .profit_un li span{display:block;font-size: 12px;color:#333;padding:inherit;}
.profit_add .profit_un p{color:#949494;}
.profitSq{background: #fff; padding:20px;}
.profitSq a{background: #09BB07;color:#fff;height: 45px;line-height: 45px;display: block;text-align:center;-webkit-border-radius: 5px;border-radius: 5px;}

.profit_main{height:45px; line-height:45px;position: relative;}
.profit_addr{display: flex;display: -webkit-box;}
.profit_addr p{padding-right: 15px;}
.profit_main span{position: absolute;top:0px;right: 0px;}
.p_bottom{margin-bottom: 10px;}
.profit_un_text{padding:10px 0;color:#969696;}